<%= form_for @room_style do |f| %>
    <%= f.hidden_field :user_id, value: current_user.id %>

    <%= f.label :style, "Name: " %>
    <%= f.text_field :style %><br>

    <%= f.label :desc, "Descripton: " %>
    <%= f.text_field :desc %><br>

    <%= f.label :cost, "Cost Multiplier: " %>
    <%= f.number_field :cost %><br>

    <%= f.submit %>
<% end %>